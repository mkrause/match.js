{
    "name": "case-match",
    "version": "2.0.0",
    "description": "JavaScript case matching library.",
    "keywords": [
        "case",
        "match",
        "pattern",
        "switch"
    ],
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/match.js",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/",
        "typings/match.d.ts"
    ],
    "es2015": "src/match.js",
    "main": "lib-cjs/match.js",
    "module": "lib-esm/match.js",
    "browser": "lib-esm/match.js",
    "types": "./typings/match.d.ts",
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --source-maps inline",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require @babel/register --recursive tests && tsd && echo '[tsd] success'",
        "prepublishOnly": "npm run build:cjs && npm run build:esm"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/register": "^7.6.2",
        "@babel/cli": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "typescript": "^3.6.4",
        "tsd": "^0.10.0",
        "mocha": "^6.2.2"
    },
    "dependencies": {
        "message-tag": "^0.4.2"
    }
}
