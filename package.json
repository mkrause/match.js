{
    "name": "@mkrause/match",
    "version": "1.0.14",
    "description": "Case matching utility.",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/match.js",
    
    "es2015": "src/match.js",
    "main": "lib-cjs/match.js",
    "module": "lib-es/match.js",
    "browser": "lib-es/match.js",
    
    "files": [
        "src/",
        "lib/",
        "tests/"
    ],
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:es": "NODE_ENV=production BABEL_ENV=es babel src --out-dir lib-es --source-maps inline",
        "build:bundle": "NODE_ENV=production webpack -p",
        "test": "mocha --recursive tests",
        
        "prepublishOnly": "npm run build:lib-cjs && npm run build:lib-es"
    },
    "devDependencies": {
        "babel-core": "^6.16.0",
        "babel-cli": "^6.24.1",
        "babel-preset-es2015": "^6.16.0",
        "babel-preset-es2016": "^6.16.0",
        "babel-preset-es2017": "^6.16.0",
        "babel-plugin-transform-object-rest-spread": "^6.20.1",
        "webpack": "^3.5.5",
        "babel-loader": "^7.1.2",
        "mocha": "^3.5.0"
    },
    "dependencies": {
        "lodash": "^4.15.0"
    }
}
