{
    env: {
        // CommonJS
        cjs: {
            presets: [
                'es2015',
                'es2016',
                'es2017',
            ],
            plugins: [
                'transform-object-rest-spread',
            ],
        },
        // ES6+ modules
        es: {
            presets: [
                ['es2015', { modules: false }],
                'es2016',
                'es2017',
            ],
            plugins: [
                'transform-object-rest-spread',
            ],
        },
    },
}
